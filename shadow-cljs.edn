{
  :source-paths ["src"]
  :dependencies [
    [medley "1.3.0"]
    [mvc-works/lilac "0.1.6"]
  ]
  :builds {
    :app {
      :target :node-script, :output-to "target/server.js", :main memof.main/main!
      :devtools {:after-load memof.main/reload!}
    }
    :testing {
      :target :node-test
      :output-to "target/test.js"
      :ns-regexp "test$"
      :autorun true
    }
    :test {
      :target :node-test
      :output-to "target/test.js"
      :ns-regexp "test$"
    }
  }
}